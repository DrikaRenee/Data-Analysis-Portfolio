--get count of genres to display which one has the highest # of songs
select count(tempo), standard_genre from dataset group by standard_genre order by count(tempo) desc;

--check for highest tempo
select max(tempo) from dataset;

--check for song associated with highest tempo
select * from dataset where tempo=243.372;

--check for correlation between loudness and tempo
select track_name, standard_genre, tempo, loudness from dataset order by loudness desc;

--loudest song
select max(loudness) from dataset;
select * from dataset where loudness=4.532;

--how many explicit vs non explicit
select standard_genre,track_name from dataset where explicit=1 group by standard_genre, track_name;
select count(explicit) from dataset where explicit=1;
